+static const unsigned icp201xx_min_freq = 1;
+static const unsigned icp201xx_max_freq[] = {
+        [ICP201XX_MODE_0] = 25,
+        [ICP201XX_MODE_1] = 120,
+        [ICP201XX_MODE_2] = 40,
+        [ICP201XX_MODE_3] = 2,
+        [ICP201XX_MODE_4] = 40,
+};
+
+
+static const char *icp201xx_mode_strings[] = {
+        [ICP201XX_MODE_0] = "0",
+        [ICP201XX_MODE_1] = "1",
+        [ICP201XX_MODE_2] = "2",
+        [ICP201XX_MODE_3] = "3",
+        [ICP201XX_MODE_4] = "4",
+};
