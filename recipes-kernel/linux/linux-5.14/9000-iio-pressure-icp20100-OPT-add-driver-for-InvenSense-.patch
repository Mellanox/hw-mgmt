+#define ICP201XX_REG_OTP_MTP_MRA_LSB		0XAF
+#define ICP201XX_REG_OTP_MTP_MRA_MSB		0XB0
+#define ICP201XX_REG_OTP_MTP_MRB_LSB		0XB1
+#define ICP201XX_REG_OTP_MTP_MRB_MSB		0XB2
